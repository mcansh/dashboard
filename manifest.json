{
  "name": "GitHub Dashboard Filter",
  "short_name": "dashboard",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Filter github.com",
  "homepage_url": "http://github.com/mcansh/github-dashboard-filter",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/",
        "https://github.com/orgs/*/dashboard"
      ],
      "css": ["dashboard.css"],
      "js": ["dashboard.js"]
    }
  ]
}
